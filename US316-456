var css = `
.ikon-test-image-div > a > img {
    width: 100%;
}
@media only screen and (max-width: 374px){
    .ikon-test-wrapper {
        font-size: 10px !important;
    }
}
.ikon-test-image-div > a {
    text-decoration: none;
    width: 100%;
}
.ikon-test-image-div > a:active {
    color: black;
}
.ikon-test-image-div > a > p {
    margin: 0;
    margin-top: 7px;
    font-family: 'Open Sans';
}
.ikon-test-wrapper {
    margin-bottom: 15px;
    margin-top: 32px;
