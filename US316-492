//////////////////////// mobile menu

megaMenuRaw.Responsive[3].ZoneMenuItems[0].SubZoneList[0].ZoneTitleName ='ERKEK BEBEK (0-6 YAŞ)';
megaMenuRaw.Responsive[3].ZoneMenuItems[0].SubZoneList[1].ZoneTitleName ='KIZ BEBEK (0-6 YAŞ)';
_uiTranslates.outlet_BabyMenuAgeDesc  = '( 0-6 Yaş )';
    }
    );
} else
{


 
///////////////////// 1. varyasyon Desktop

