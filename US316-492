{
    document.addEventListener("DOMContentLoaded", function ()
    {
      
///////////////////// 1. varyasyon Desktop

megaMenuRaw.Desktop[3].ZoneMenuItems[0].SubZoneList[0].ZoneTitleName ='ERKEK BEBEK (0-6 YAŞ)';
