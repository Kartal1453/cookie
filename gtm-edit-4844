window.onload=function(){setTimeout(function(){dom_observer.disconnect();console.log("load")},1E3)};
var dom_observer=new MutationObserver(function(b){!!document.querySelector(".cart-item") && !!document.querySelector(".cart-item").querySelector("img")&&document.querySelectorAll(".cart-item").forEach(function(a){a.querySelector("img").srcset.toLocaleUpperCase().includes(productCode)&&!google_tag_manager["GTM-P7BHVG"].dataLayer.get("4844")&&(dataLayer.push({event:"optimize.activate4844",4844:"true"}),dom_observer.disconnect())})}),container=document.body,configs={subtree:!0,attributes:!1,childList:!0,characterData:!1};
dom_observer.observe(container,configs);
